        <Button x:Name="btnhome" Content="Home" HorizontalAlignment="Left" Margin="47,43,0,0" VerticalAlignment="Top" Width="75" Height="36" Click="btnhome_Click"/>
        <Button x:Name="btncur" Content="Curriculum" HorizontalAlignment="Left" Margin="537,43,0,0" VerticalAlignment="Top" Width="75" Height="36"/>
        <Button x:Name="btnsetSy" Content="Set School year" HorizontalAlignment="Left" Margin="632,43,0,0" VerticalAlignment="Top" Width="99" Height="36" Click="btnsetSy_Click"/>


    </Grid>
</Window>
