        xmlns:local="clr-namespace:IloiloCityCommunityCollege"
        mc:Ignorable="d"
        Title="windowMain" Height="768" Width="1366" WindowState="Maximized" ResizeMode="NoResize" WindowStyle="None">

    <Grid>
