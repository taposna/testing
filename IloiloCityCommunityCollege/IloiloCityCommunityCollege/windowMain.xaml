        <Button x:Name="stubtn" Content="Student" HorizontalAlignment="Left" Margin="147,43,0,0" VerticalAlignment="Top" Width="74" Height="36" Click="stubtn_Click"/>
        <Button x:Name="sched" Content="Class Schedule" HorizontalAlignment="Left" Margin="236,43,0,0" VerticalAlignment="Top" Width="91" Height="36"/>
